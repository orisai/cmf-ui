{templateType OriCMF\UI\Control\Breadcrumb\BreadcrumbTemplate}
<nav>
	<ol class="breadcrumb">
		<li n:foreach="$links as $link" {if !$link['link']}class="active"{/if}>
			<i class="mdi mdi-{$link['icon']}" n:if="$link['icon']"></i>
			{if $link['link']}
				<a href="{$link['link']}" title="{$link['title']}">{$link['title']}</a>
			{else}
				{$link['title']}
			{/if}
			<i class="icon-angle-right" n:if="!$iterator->isLast()"></i>
		</li>
	</ol>
</nav>
