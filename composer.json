{
	"name": "orisai/cmf-ui",
	"description": "UI module for Orisai CMF",
	"keywords": [
		"cmf",
		"controller",
		"form",
		"forms",
		"latte",
		"nette",
		"orisai",
		"presenter",
		"templates",
		"themes",
		"ui"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/cmf-ui",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=8.0.0",
		"latte/latte": "^2.10.0",
		"nepada/presenter-mapping": "^2.3.0",
		"nette/application": "^3.1.0",
		"nette/forms": "^3.1.0",
		"nette/http": "^3.1.0",
		"orisai/cmf-core": "^1.0.0"
	},
	"require-dev": {
		"infection/infection": "^0.22.1",
		"nextras/orm-phpstan": "^0.6.2",
		"orisai/coding-standard": "^2.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-deprecation-rules": "^0.12.0",
		"phpstan/phpstan-nette": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"phpstan/phpstan-strict-rules": "^0.12.0",
		"phpunit/phpunit": "^9.3.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.1.0"
	},
	"autoload": {
		"psr-4": {
			"OriCMF\\UI\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\OriCMF\\UI\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	}
}
